<div class="modal-dialog" role="document">
   <div class="modal-content">
      <div class="modal-header">
         <h4 class="modal-title text-center">Add users to project</h4>
      </div>
      <div class="modal-body">
         <div class="row">
            <div class="col-md-6">
               <%= bootstrap_form_tag  url: "/projects/#{@project.id}/search_users" do |f| %>
                  <%= f.hidden_field :id, value: @project.id %>
                  <%= f.text_field :search, append: f.submit('Search'), input_group_class: 'input-group-sm' %>
               <% end %>
            </div>
         </div>
         <hr>
         <div class="row">
            <div class="col-xs-12 table-responsive">
               <table class="table">
                  <thead>
                     <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th> </th>
                     </tr>
                  </thead>
                  <tbody>

                  </tbody>
               </table>
            </div>
         </div>
      </div>
      <div class="modal-footer">
         <button type="button" class="btn btn-info" data-dismiss="modal">Close</button>
      </div>
   </div>
</div>
